{
  "manifest_version": 3,
  "name": "VueTools",
  "version": "1.0",
  "description": "Injects JavaScript into a webpage.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
